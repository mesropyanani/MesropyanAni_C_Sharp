@{
	ViewBag.Title = "List";
}
<h2>My Guestbook Entries</h2>
<p>
	<a href="/Guestbook/Create">Add a new entry</a>
</p>
@foreach (var entry in ViewBag.gg)
{
	<section class="contact">
		<header>
			<h3>@entry.Message</h3>
		</header>
		<p>
			Posted by <h2> @entry.Name </h2> on @entry.DateAdded.ToLongDateString()
		</p>
	</section>
}

